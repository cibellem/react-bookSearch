{
  "name": "react-book-search",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.13.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "build": "cd client && npm run CI",
    "client": "cd client && npm run start",
    "install": "cd client && npm install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.7",
    "set-value": ">=4.0.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.13.11",
    "react-router-dom": "^5.3.0"
  }
}
